<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.healthapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".clientoptions"
            android:label="@string/title_activity_clientoptions" >
        </activity>
        <activity
            android:name=".calculateBmi"
            android:label="@string/title_activity_calculate_bmi" >
        </activity>
        <activity
            android:name=".calculate_calories"
            android:label="@string/title_activity_calculate_calories" >
        </activity>
        <activity
            android:name=".Calories_Burned"
            android:label="@string/title_activity_calories__burned" >
        </activity>
        <activity
            android:name=".SetTarget"
            android:label="@string/title_activity_set_target" >
        </activity>
    </application>

</manifest>
